{
  "name": "tag-recon",
  "version": "0.1.0",
  "description": "OSINT website analytics and advertising tracking codes for discovering connections",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint . --fix",
    "test": "ava --verbose"
  },
  "keywords": [
    "scrape",
    "osint",
    "recon",
    "builtwith",
    "spyonweb"
  ],
  "author": {
    "name": "Daehee Park",
    "email": "hello@daehee.com",
    "url": "https://github.com/daehee/"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "lodash": "^4.17.15",
    "x-ray": "^2.3.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "semi": "error"
    }
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "eslint": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "xo": "^0.24.0"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "env": [
      "node"
    ],
    "rules": {
      "complexity": [
        "error",
        38
      ]
    }
  }
}
